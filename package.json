{
  "name": "avans-2.2-spa",
  "version": "1.0.0",
  "description": "Single Page WebApplication for 2.2 Avans made in react",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "client": "npm start --prefix client",
    "server": "nodemon server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "test": "node node_modules/mocha/bin/_mocha ./tests/* --reporter spec --exit",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false  npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RicardovdKruisweg/avans-2.2-SPA.git"
  },
  "author": "Ricardo van de Kruisweg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RicardovdKruisweg/avans-2.2-SPA/issues"
  },
  "homepage": "https://github.com/RicardovdKruisweg/avans-2.2-SPA#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "mongoose": "^5.3.14"
  },
  "devDependencies": {
    "nodemon": "^1.18.7"
  }
}
